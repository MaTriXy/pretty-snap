{
    "version": "1.0.0",
    "name": "boilerplate-preact",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npm run dev",
        "dev": "npm run typecheck && npm run parcel:watch",
        "build": "npm run typecheck && npm run parcel:build && npm run static-assets",
        "parcel:watch": "parcel ./src/index.html",
        "parcel:build": "parcel build ./src/index.html --no-source-maps",
        "static-assets": "node copy.js",
        "typecheck": "tsc --noEmit"
    },
    "alias": {
        "react": "preact/compat",
        "react-dom": "preact/compat"
    },
    "browserslist": [
        "last 2 chrome versions",
        "last 2 firefox versions"
    ],
    "devDependencies": {
        "@types/dom-to-image": "^2.6.2",
        "@types/react": "^16.9.55",
        "autoprefixer": "^9.8.6",
        "fs-extra": "^9.0.1",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-bundle-visualiser": "^1.2.0",
        "tailwindcss": "^1.9.5",
        "typescript": "^4.0.3"
    },
    "dependencies": {
        "dom-to-image": "^2.6.0",
        "preact": "^10.5.5",
        "react-merge-refs": "^1.1.0",
        "react-spring": "^8.0.27",
        "react-use-measure": "^2.0.2",
        "zustand": "^3.1.3"
    }
}